root = true

[*]
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = true

[*.java]
indent_style = space
indent_size = 2

[*.sh]
end_of_line = lf
indent_style = space
indent_size = 2
insert_final_newline = true

[*.fhir.json]
insert_final_newline = unset

[dq/**/*.json]
insert_final_newline = unset
